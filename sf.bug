model
{
    ij <- (j - 1) * ni + i
    for (m in 1:M) {
        a[m] <- H[m, ij]
        c[m] <- a[m] * q
        mu[m] ~ dnorm(c[m], tau[m])
    }
    Mj ~ dcat(pj[])
    j <- jCat[Mj]
    y <- yc[j]
    Mi ~ dcat(pij[1:ni, j])
    i <- iCat[Mi]
    x <- xc[i]
    log.q ~ dunif(logqLower, logqUpper)
    q <- 10^log.q
}
