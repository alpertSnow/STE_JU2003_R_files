model
{
    ijk <- (k - 1) * ni * nj + (j - 1) * ni + i
    for (m in 1:M) {
        a[m] <- H[m, ijk]
        c[m] <- a[m] * q
        mu[m] ~ dnorm(c[m], tau[m])
        mu[m] ~ dt(c[m], tau[m], 2)
    }
    Mk ~ dcat(pk[])
    k <- kCat[Mk]
    z <- zc[k]
    Mj ~ dcat(pjk[1:nj, k])
    j <- jCat[Mj]
    y <- yc[j]
    Mi ~ dcat(pijk[1:ni, (k - 1) * nj + j])
    i <- iCat[Mi]
    x <- xc[i]
    log.q ~ dunif(logqLower, logqUpper)
    q <- 10^log.q
}
